 کوتاه ترین زمان به ما بسپارید⚜️\n\n⬅️آدرس : خیابان انقلاب ، بعد از خیابان رازی ، قبل از آراسته اول کوچه قاسم آزادی املاک خسروانی مرکزی", "is_primary": true}}]}, {"section_name": "STATIC", "widgets": [{"widget_type": "SECTION_DIVIDER_ROW", "data": {"@type": "type.googleapis.com/widgets.SectionDividerRowData", "padded": true}}, {"widget_type": "SELECTOR_ROW", "data": {"@type": "type.googleapis.com/widgets.SelectorRowData", "title": "گزارش آگهی", "action": {"type": "OPEN_SCHEMA_PAGE", "payload": {"@type": "type.googleapis.com/widgets.OpenSchemaPagePayload", "request_path": "/v8/post-quality/feedback-v2", "additional_data": {"@type": "type.googleapis.com/widgets.GetFeedbackPageRequest.AdditionalData", "post_token": "gZonSs4b"}, "is_modal": true, "rest_request_path": "/v8/post-quality/feedback-v2", "grpc_request_path": "/post_quality.PostQualityFeedback/GetFeedbackPage"}}, "icon": {"image_url_dark": "https://s100.divarcdn.com/static/imgs/widget-icons/dark/icon_secondary/v1/report.png", "image_url_light": "https://s100.divarcdn.com/static/imgs/widget-icons/light/icon_secondary/v1/report.png", "icon_name": "REPORT", "icon_color": "ICON_SECONDARY"}, "has_arrow": true, "last_notification_date": "0001-01-01T00:00:00Z"}}]}, {"section_name": "TAGS", "widgets": [{"widget_type": "WRAPPER_ROW", "data": {"@type": "type.googleapis.com/widgets.WrapperRowData", "chip_list": {"chips": [{"text": "فروش خانه و ویلا", "type": "ACTION", "action": {"type": "OPEN_POSTLIST_PAGE", "payload": {"@type": "type.googleapis.com/widgets.OpenPostListPagePayload", "web_url": "khorramabad/buy-villa"}}}]}}}]}, {"section_name": "MAP", "widgets": [{"widget_type": "MAP_ROW", "data": {"@type": "type.googleapis.com/widgets.MapRowData", "location": {"exact_data": {"point": {"latitude": 33.505043, "longitude": 48.35843}}}, "image_url": "https://api.divar.ir/v8/mapimage?encrypted_data=MTIzNDU2Nzg5MTIzXDFm0q77163uQthE6ynP9vtLnbXL97IiBnRfNDFnFJ1Tgg=="}}]}, {"section_name": "BUSINESS_SECTION", "widgets": [{"widget_